    integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60="
    crossorigin="anonymous">
  </script>
{% block script %}
{% if script %}
  <script src="/javascript/{{ script }}?v={{ nocache }}"></script>
{% endif %}
{% endblock %}
<title>{{ site.title }}{% if title %} | {{ title }}{% endif %}</title>

  </head>
