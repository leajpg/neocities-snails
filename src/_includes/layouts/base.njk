            <!-- CSS/JS + CACHEBUSTING --->

            {% if site.domain == "snails.nekoweb.org" %}
            <link rel="stylesheet" href="css/styles.css?nocache={{ nocache }}">
          {% else %}
            <link rel="stylesheet" href="/css/styles.css?nocache={{ nocache }}">
          {% endif %}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.css">
